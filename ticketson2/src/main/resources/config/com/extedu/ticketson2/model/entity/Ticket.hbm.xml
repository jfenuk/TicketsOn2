<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.extedu.ticketson2.model.entity.Ticket" table="TICKET">
        <id column="ID" name="id" unsaved-value="0">
            <generator class="increment"/>
        </id>
        <property column="TITLE" name="title" not-null="true"/>
        <property column="DESCRIPTION" name="description" not-null="true"/>        
    </class>
</hibernate-mapping>
